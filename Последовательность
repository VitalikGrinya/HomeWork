using System.Text;
using System.Threading.Tasks;

namespace CSLight
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int firstNumber = -2;
            int differenceInNumbers = 4;
            int lastNumber = 96;

            for(int i = 0; i++ < lastNumber; i++)
            {
                i += differenceInNumbers;
                Console.WriteLine(i++);
            }
        }
    }
}
