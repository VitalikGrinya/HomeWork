using System.Text;
using System.Threading.Tasks;

namespace CSLight
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int firstNumber = 5;
            int differenceInNumbers = 7;
            int lastNumber = 96;

            for(int i = firstNumber; i <= lastNumber; i+=differenceInNumbers)
            {
                Console.WriteLine(i);
            }
        }
    }
}
